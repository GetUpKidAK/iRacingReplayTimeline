<Application x:Class="ReplayTimeline.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ReplayTimeline"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
		<local:BoolToVisibilityConverter x:Key="VisibilityConverter"/>
		<local:InvertedBoolConverter x:Key="InvertedBoolConverter"/>
		<local:FlexibleBooleanToVisibilityConverter x:Key="FlexibleBooleanToVisibilityConverter"/>
		<local:SecondsToTimeConverter x:Key="SecondsToTimeConverter"/>
	</Application.Resources>
</Application>
